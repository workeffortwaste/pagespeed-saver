{
  "manifest_version": 3,
  "name": "PageSpeed Saver",
  "version": "2.1.0",
  "icons": { "128": "icon128.png" },
  "description": "Quickly save your PageSpeed Insights reports to JSON.",
  "content_scripts": [
    {
      "matches": ["https://pagespeed.web.dev/*"],
      "js": ["index.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["bundle.js"],
      "matches": ["https://pagespeed.web.dev/*"]
    }
  ]
}